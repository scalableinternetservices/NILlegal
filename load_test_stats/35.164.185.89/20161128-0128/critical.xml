<?xml version="1.0"?><tsung loglevel="notice" version="1.0">

  <clients>
    <client host="localhost" use_controller_vm="true" maxusers="15000"/>
  </clients>

  <servers>
    <server host="ec2-35-164-235-251.us-west-2.compute.amazonaws.com" port="80" type="tcp"/>
  </servers>


  <load>
    <arrivalphase phase="1" duration="20" unit="second">
      <users arrivalrate="1" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="2" duration="20" unit="second">
      <users arrivalrate="2" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="3" duration="20" unit="second">
      <users arrivalrate="4" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="4" duration="20" unit="second">
      <users arrivalrate="5" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="5" duration="20" unit="second">
      <users arrivalrate="6" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="6" duration="20" unit="second">
      <users arrivalrate="7" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="7" duration="20" unit="second">
      <users arrivalrate="8" unit="second"/>
    </arrivalphase>
    <arrivalphase phase="8" duration="20" unit="second">
      <users arrivalrate="10" unit="second"/>
    </arrivalphase>
    </load>

  <options>
    <option name="global_ack_timeout" value="2000"/>

    <option type="ts_http" name="user_agent">
      <user_agent probability="100">Mozilla/5.0 (Windows; U; Windows NT 5.2; fr-FR; rv:1.7.8) Gecko/20050511 Firefox/1.0.4</user_agent>
    </option>
  </options>

  <sessions>
    <session name="http-example" probability="100" type="ts_http">
      <request>
        <http url="/" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <request>
        <http url="/users/sign_up" version="1.1" method="GET"/>
      </request>

      <setdynvars sourcetype="random_number" start="1000" end="9999999">
        <var name="random_user_number"/>
      </setdynvars>

      <request subst="true">
        <http url="/users" version="1.1" method="POST" contents="user%5Bname%5D=User_%%_random_user_number%%&amp;user%5Bemail%5D=u_%%_random_user_number%%%40foo.com&amp;user%5Bpassword%5D=12341234&amp;user%5Bpassword_confirmation%5D=12341234&amp;commit=Sign+up">
        </http>
      </request>

      <request>
        <http url="/dares" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <request>
        <http url="/dares/new" version="1.1" method="GET"/>
      </request>

      <thinktime value="2" random="true"/>

      <setdynvars sourcetype="random_number" start="1000" end="9999999">
        <var name="dare_title"/>
      </setdynvars>

      <request subst="true">
        <http url="/dares" version="1.1" method="POST" contents="dare%5Btitle%5D=%%_dare_title%%&amp;dare%5Bkarma_offer%5D=3&amp;dare%5Bdescription%5D=some+dare+description&amp;commit=Post+Dare">
        </http>
      </request>

      <thinktime value="2" random="true"/>

      <setdynvars sourcetype="random_number" start="1" end="50">
        <var name="random_dare_id"/>
      </setdynvars>

      <request subst="true">
        <http url="/dares/%%_random_dare_id%%" version="1.1" method="GET"/>
      </request>

      <request subst="true">
        <http url="/dare_submissions/new?dare_submission%5Bdare_id%5D=%%_random_dare_id%%" version="1.1" method="GET">
        </http>
      </request>

      <thinktime value="2" random="true"/>

      <request subst="true">
        <http url="/dare_submissions" version="1.1" method="POST" contents="dare_submission%5Bcontent%5D=https%3A%2F%2Fwww.youtube.com%2Fwatch%3Fv%3DoHmvI76XciU&amp;dare_submission%5Bdescription%5D=This+is+the+best+video%21&amp;dare_submission%5Bdare_id%5D=%%_random_dare_id%%&amp;commit=Post+Dare+Submission">
        </http>
      </request>

      <request subst="true">
        <http url="/comments" version="1.1" method="POST" contents="comment%5Bbody%5D=some+text+for+comment&amp;comment%5Bdare_id%5D=%%_random_dare_id%%&amp;commit=Comment">
        </http>
      </request>

      <request subst="true">
        <http url="/leaderboard" version="1.1" method="GET">
        </http>
      </request>

      <thinktime value="2" random="true"/>
    </session>
  </sessions>
</tsung>
